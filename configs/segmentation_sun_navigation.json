{
  "exp": {
    "name": "fast_scnn_navigation"
  },
  "generator": {
    "img_dir": "datasets/sun_rgbd/SUNRGBD-train_images",
    "mask_dir": "datasets/sun_navigation/train_mask",
    "depth_dir": null,
    "use_data_augmentation": true,
    "shuffle_seed": 9
  },
  "validation": {
    "img_dir": "datasets/sun_rgbd/SUNRGBD-test_images",
    "mask_dir": "datasets/sun_navigation/test_mask",
    "depth_dir": null,
    "weights_file": "experiments/fast_scnn_navigation-71.hdf5"
  },
  "model": {
    "class_name": "models.fast_scnn_sun_navigation.FastScnnSunNavigation",
    "optimizer": "Adam",
    "learning_rate": 0.001,
    "beta_1": 0.9,
    "beta_2": 0.999,
    "momentum": 0.9,
    "width": 730,
    "height": 530,
    "classes": 5,
    "alpha": 1.0,
    "gamma": 2.0,
    "loss": "focal_loss"
  },
  "trainer": {
    "num_epochs": 100,
    "batch_size": 12,
    "verbose_training": true,
    "workers": 1
  },
  "callbacks": {
    "checkpoint_monitor": "val_loss",
    "checkpoint_mode": "min",
    "checkpoint_save_best_only": true,
    "checkpoint_save_weights_only": true,
    "checkpoint_verbose": true,
    "tensorboard_write_graph": true
  }
}